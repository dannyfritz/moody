{
  "name": "moody",
  "version": "0.3.0",
  "description": "Moody is a library for defining running states such as a gamestate.",
  "main": "index.js",
	"typings": "dist/index.d.ts",
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "build": "typings install && tsc",
    "dev": "typings install && tsc --watch",
    "test": "npm run lint && npm run unittest",
    "lint": "eslint lib",
    "unittest": "npm run build && tape test/*.js | tap-spec"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "coverify": "^1.0.7",
    "eslint": "^3.3.1",
    "sinon": "^1.10.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "tsify": "^1.0.5",
    "typescript": "^1.8.10",
    "typescript-eslint-parser": "^0.2.0",
    "typings": "^1.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://dannyfritz@github.com/dannyfritz/moody.git"
  },
  "keywords": [
    "gamestate",
    "state"
  ],
  "author": "Danny Fritz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dannyfritz/moody/issues"
  },
  "homepage": "https://github.com/dannyfritz/moody"
