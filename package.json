{
  "name": "moody",
  "version": "0.3.0",
  "description": "Moody is a library for defining running states such as a gamestate.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "npm run lint && npm run unittest",
    "lint": "eslint lib --ext .ts --ext .js",
    "unittest": "npm run build && tape test/*.js | tap-spec"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.34",
    "eslint": "^3.3.1",
    "eslint-plugin-typescript": "^0.1.0",
    "sinon": "^1.10.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "typescript": "^2.0.0",
    "typescript-eslint-parser": "^0.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://dannyfritz@github.com/dannyfritz/moody.git"
  },
  "keywords": [
    "gamestate",
    "state"
  ],
  "author": "Danny Fritz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dannyfritz/moody/issues"
  },
  "homepage": "https://github.com/dannyfritz/moody"
}
